[access]
	inheritFrom = All-Projects
[project]
	description = test only
[access "refs/heads/*"]
	pushMerge = group gerritTest reviewers
	pushMerge = group replication users
	label-Code-Review = -2..+2 group authorized users
	label-Code-Review = -2..+2 group gerritTest reviewers
	label-Code-Review = -2..+2 group replication users
	submit = group authorized users
	create = group gerritTest reviewers
	create = group replication users
	push = group gerritTest reviewers
	push = group replication users
	label-Verified = -1..+1 group gerritTest reviewers
[label "Verified"]
       function = MaxWithBlock
       value = -1 Fails
       value =  0 No score
       value = +1 Verified
       defaultValue = 0